  <requires>
    <import addon="xbmc.python" version="3.0.0" />
    <import addon="script.module.requests" version="2.22.0" optional="true" />
  </requires>

  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>

  <extension point="xbmc.service" library="service.py" start="login" />

  <extension point="xbmc.addon.metadata">
    <summary>Browse RetroAchievements profile, recent unlocks, and people you follow</summary>
    <description>RetroAchievements client for Kodi. Requires your RA username and Web API key from your account page.</description>
    <platform>all</platform>
    <license>MIT</license>
    <source>https://retroachievements.org/</source>
    <assets>
      <icon>icon.png</icon>
      <fanart>fanart.png</fanart>
    </assets>
  </extension>
