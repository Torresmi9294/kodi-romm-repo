</ns0:addon><addon id="script.program.romm.launcher" name="RomM Launcher (Omega)" version="0.15.3.1" provider-name="Omega RomM Master">
  <requires>
    <import addon="xbmc.python" version="3.0.1" />
  </requires>
  <extension point="xbmc.python.script" library="runner.py" />
  <extension point="xbmc.python.pluginsource" library="main.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">Browse your RomM library, download games, and launch via RetroArch.</summary>
    <description lang="en_GB">
      New: Custom poster/thumb for "Next Page" items in game lists via resources/media/next_page.png (or next.png, next-page.png, more.png).
      Fast view switching with micro re-apply; media folder support for Store/Play/Platforms/All Installed.
    </description>
    <platform>all</platform>
    <license>MIT</license>
    <reuselanguageinvoker>false</reuselanguageinvoker>
  </extension>
